col>\n              <v-col cols=\"12\" sm=\"6\">\n                <v-text-field\n                  v-model=\"newForm.cab\"\n                  label=\"Кабинет*\"\n                  :rules=\"nameRules\"\n                  required\n                ></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n          <small>*Обязательные поля</small>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\"\n            >Закрыть</v-btn\n          >\n          <v-btn color=\"blue darken-1\" text @click=\"validate\">Отправить</v-btn>\n        </v-card-actions>\n      </v-form>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport { bus } from \"../bus\";\nexport default {\n  name: \"App\",\n  components: {},\n  props: {\n    source: String,\n  },\n  data: () => ({\n    newForm: {\n      firstname: \"\",\n      lastname: \"\",\n      cab: \"\",\n      problem: \"\",\n      sp: \"\",\n    },\n\n    drawer: true,\n    dialog: false,\n    items: [\"СП-1\", \"СП-2\", \"СП-3\", \"СП-4\", \"СП-5\"],\n    nameRules: [(v) => !!v || \"Имя обязательно\"],\n    lastNameRules: [(v) => !!v || \"Фамилия обязательна\"],\n  }),\n\n  methods: {\n    async validate() {\n      if (this.$refs.form.validate()) {\n        this.dialog = !this.dialog;\n        let data = {\n          \n        };\n        console.log(\"modal\");\n        bus.$emit(\"create-Todo\", data);\n        // bus.$emit(\"create-Todo\", data);\n      }\n    },\n  },\n};\n</script>\n"]}]}