.status == 3\">\n          mdi-check\n        </v-icon>\n        <v-icon color=\"gray\" size=\"22\" v-if=\"todo.status == 1\">\n          mdi-clock-outline\n        </v-icon>\n      </template>\n    </v-expansion-panel-header>\n    <v-expansion-panel-content>\n      <span class=\"d-block pt-4\">\n        {{ todo.text }}\n      </span>\n      <v-row class=\"mt-3\">\n        <v-btn\n          height=\"35\"\n          class=\"ma-2 ml-auto\"\n          width=\"150\"\n          v-if=\"todo.status == 2\"\n          color=\"green\"\n          dark\n          @click.prevent=\"$emit('finish-todo', [todo.id, count])\"\n        >\n          Завершить\n          <v-icon dark right> mdi-checkbox-marked-circle </v-icon>\n        </v-btn>\n        <v-btn\n          height=\"35\"\n          width=\"150\"\n          v-if=\"todo.status == 1\"\n          class=\"ma-2 ml-auto\"\n          color=\"#23539c\"\n          dark\n          @click.prevent=\"$emit('start-todo', [todo.id, count])\"\n        >\n          Принять\n          <v-icon dark right> mdi-text-box-check </v-icon>\n        </v-btn>\n        <v-btn\n          height=\"35\"\n          v-if=\"todo.status == 3\"\n          width=\"150\"\n          class=\"ma-2 ml-auto\"\n          color=\"red\"\n          dark\n          @click.prevent=\"$emit('del-todo', [todo.id, count])\"\n        >\n          удалить\n          <v-icon dark right> mdi-delete </v-icon>\n        </v-btn>\n      </v-row>\n    </v-expansion-panel-content>\n  </v-expansion-panel>\n</template>\n\n<script>\nimport dateFormat from \"dateformat\";\nexport default {\n  name: \"App\",\n  props: {\n    todo: {\n      date: \"\",\n      sp: \"\",\n      cab: \"\",\n      user: \"\",\n      text: \"\",\n      status: \"\",\n    },\n    count: null,\n  },\n  methods: {\n    dateupdate(todo) {\n      // var dateFormat = require(\"dateformat\");\n      return (\n        dateFormat(todo, \"dd\") +\n        \".\" +\n        dateFormat(todo, \"mm\") +\n        \".\" +\n        dateFormat(todo, \"yy\")\n      );\n    },\n  },\n};\n</script>\n"]}]}